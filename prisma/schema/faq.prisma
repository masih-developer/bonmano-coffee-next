model faq {
  id        Int      @id @default(autoincrement())
  question  String
  answer    String
  productId Int
  createdAt DateTime @default(now())
  updatedAt DateTime
  product   product  @relation(fields: [productId], references: [id], map: "Faq_productId_fkey")

  @@index([productId], map: "Faq_productId_idx")
}
